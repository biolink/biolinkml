meta:Definition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "definition" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:abstract ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Definition ;
                        owl:onProperty meta:apply_to ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Definition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:is_a ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:onProperty meta:local_names ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:mixin ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Definition ;
                        owl:onProperty meta:mixins ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:onProperty meta:values_from ] ) ],
        meta:Element ;
    skos:definition "base class for definitions" .

meta:TypeDefinition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "type definition" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:base ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:uri ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:TypeDefinition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:typeof ] ) ],
        meta:Element ;
    skos:definition "A data type definition." .

meta:Element a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "element" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:onProperty meta:aliases ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:onProperty meta:comments ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:deprecated ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:description ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Example ;
                        owl:onProperty meta:examples ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SchemaDefinition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:from_schema ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:ncname ;
                        owl:onProperty meta:id_prefixes ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SubsetDefinition ;
                        owl:onProperty meta:in_subset ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:onProperty meta:mappings ] [ a owl:Restriction ;
                        owl:cardinality 1 ;
                        owl:onProperty meta:name ;
                        owl:someValuesFrom meta:string ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:onProperty meta:notes ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:onProperty meta:see_also ] ) ] ;
    skos:definition "a named element in the model" .

meta:SchemaDefinition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "schema definition" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:ClassDefinition ;
                        owl:onProperty meta:classes ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:onProperty meta:default_curi_maps ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:default_prefix ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Definition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:default_range ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:datetime ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:generation_date ] [ a owl:Restriction ;
                        owl:cardinality 1 ;
                        owl:onProperty meta:id ;
                        owl:someValuesFrom meta:uri ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:onProperty meta:imports ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:license ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:metamodel_version ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Prefix ;
                        owl:onProperty meta:prefixes ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:description ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:slots ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:source_file ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:datetime ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:source_file_date ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:integer ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:source_file_size ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SubsetDefinition ;
                        owl:onProperty meta:subsets ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:title ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:TypeDefinition ;
                        owl:onProperty meta:types ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:version ] ) ],
        meta:Element ;
    skos:definition "a collection of subset, type, slot and class definitions" .

meta:uri a meta:TypeDefinition,
        owl:Class ;
    rdfs:label "uri" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "a URI or a CURIE" .

meta:ClassDefinition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "class definition" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:ClassDefinition ;
                        owl:onProperty meta:apply_to ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:ClassDefinition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:is_a ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:ClassDefinition ;
                        owl:onProperty meta:mixins ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:class_uri ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:defining_slots ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:slot_usage ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:slots ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:subclass_of ] ) ],
        meta:Definition ;
    skos:definition "the definition of a class or interface" .

meta:string a meta:TypeDefinition,
        owl:Class ;
    rdfs:label "string" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] .

meta:SlotDefinition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "slot definition" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:alias ] [ a owl:Restriction ;
                        owl:cardinality 1 ;
                        owl:onProperty meta:domain ;
                        owl:someValuesFrom meta:ClassDefinition ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:identifier ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:ifabsent ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:inherited ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:inlined ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:inverse ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:key ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:multivalued ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:Element ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:range ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:subclass_of ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:readonly ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:boolean ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:required ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:apply_to ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:is_a ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:SlotDefinition ;
                        owl:onProperty meta:mixins ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:uri ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:slot_uri ] ) ],
        meta:Definition ;
    skos:definition "the definition of a property or a slot" .



