meta:integer a meta:TypeDefinition,
        owl:Class ;
    rdfs:label "integer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:integer ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] .

meta:slots a meta:SlotDefinition,
        owl:AnnotationProperty ;
    rdfs:label "slots" ;
    rdfs:domain meta:ClassDefinition ;
    rdfs:range meta:SlotDefinition ;
    skos:definition "list of slot names that are applicable to a class" ;
    skos:note "the preferred way for defining slots is through the slot domain. The final list of slots for a class is the combination of the parent (is_a) slots, mixins slots, apply_to slots minus the slot_usage entries." .

meta:subclass_of a meta:SlotDefinition,
        owl:AnnotationProperty ;
    rdfs:label "subclass_of" ;
    rdfs:domain meta:ClassDefinition ;
    rdfs:range meta:uri ;
    skos:definition "instances of this class are considered/required to be sub classes of the target URI" .

meta:Example a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "example" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:value ] [ a owl:Restriction ;
                        owl:allValuesFrom meta:string ;
                        owl:maxCardinality 1 ;
                        owl:onProperty meta:description ] ) ] ;
    skos:definition "usage example and description" .

meta:Prefix a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "prefix" ;
    rdfs:subClassOf [ owl:unionOf ( [ a owl:Restriction ;
                        owl:cardinality 1 ;
                        owl:onProperty meta:local_name ;
                        owl:someValuesFrom meta:ncname ] [ a owl:Restriction ;
                        owl:cardinality 1 ;
                        owl:onProperty meta:prefix_uri ;
                        owl:someValuesFrom meta:uri ] ) ] ;
    skos:definition "prefix URI tuple" .

meta:SubsetDefinition a meta:ClassDefinition,
        owl:Class ;
    rdfs:label "subset definition" ;
